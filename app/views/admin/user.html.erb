<%= render(:partial=>'admin')%>
<br /><br />
<div class="admin-form">
  <fieldset>
    <legend>Manage Users</legend>
    <br />
    <table>
      <tr class="header">
        <td>User #</td>
        <td>Email</td>
        <td>Options</td>
      </tr>
      <%= render :partial => 'user', :collection => @display_data %>
    </table>
    <div class = "footer-controls">
        <%= link_to image_tag('/images/add.png', :size => '16x16'), {:controller => 'account', :action =>'signup' } %>&nbsp;Add New User
    </div>
    <div class="pagination">
       <%= will_paginate @display_data %>  
    </div>
  </fieldset>
</div>